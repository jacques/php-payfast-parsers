{
    "name": "jacques/php-payfast-parsers",
    "description": "Parsers for PayFast Reconciliation Files",
    "repositories": [
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "league/csv": "^9.5",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpmd/phpmd": "^2.8",
        "phpro/grumphp": "^0.17.2",
        "phpunit/php-invoker": "^3.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\PayFast\\Parsers\\": "src/"
        }
    },
    "license": "mpl",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@powertrip.co.za"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
