{
    "name": "jacques/php-payfast-parsers",
    "description": "Parsers for PayFast Reconciliation Files",
    "repositories": [
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "league/csv": "^8.2",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\PayFast\\Parsers\\": "src/"
        }
    },
    "license": "mpl",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@powertrip.co.za"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
